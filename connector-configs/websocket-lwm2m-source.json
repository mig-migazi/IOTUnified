{
  "name": "websocket-lwm2m-source",
  "config": {
    "connector.class": "io.confluent.connect.websocket.WebSocketSourceConnector",
    "websocket.uri": "ws://lwm2m-server:8080/socket.io/?EIO=4&transport=websocket",
    "websocket.topic": "device_event",
    "kafka.topic": "iot.telemetry.lwm2m.websocket",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "tasks.max": "1",
    "websocket.client.id": "redpanda-connect-lwm2m",
    "websocket.reconnect.interval.ms": "5000",
    "websocket.connection.timeout.ms": "10000"
  }
} 